<component
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/component/1.1.1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/component/1.1.1 http://maven.apache.org/xsd/component-1.1.1.xsd">
	<dependencySets>
		<dependencySet>
			<outputDirectory>lib</outputDirectory>
			<includes>
				<include>org.cipollino:cipollino-agent</include>
				<include>org.cipollino:cipollino-core</include>
				<include>org.cipollino:cipollino-logger</include>
			</includes>
			<useTransitiveDependencies>true</useTransitiveDependencies>
			<useTransitiveFiltering>true</useTransitiveFiltering>
			<useProjectArtifact>false</useProjectArtifact>
		</dependencySet>
	</dependencySets>

</component>
